
include
	.js('http://code.jquery.com/jquery-2.1.0.min.js')
	.done(function(){
		
		A.assert.setDOMLibrary(jQuery);
		
		UTest({
			'jquery': function(){
				
				var dom = mask.render('div > h4 > span > "Hello"');
				
				var error;
				try {
					$(dom)
						.find('h4')
						.$eq('length', 1)
						.$eq('text', 'Hello')
						.$has('html', 'span')
						
						.end()
						.$has('div')
						.$has('span')
						;
				} catch (e) {
					error = e;
				}
				eq(error, null);
				
				var assume = assert.await();
				try {
					
					$(dom)
						.find('h4')
						.$eq(['length', 2], 'foo')
						;
				}
				catch(error) {
					
					assume();
					assert.has(error.message, 'foo');
				}
				
				
				assume = assert.await('has no input');
				error = null;
				try {
					
					$(dom)
						.$has(['span', 2], 'bazput')
						;
				}
				catch(e) {
					
					assume();
					assert.eq(e.message, 'bazput');
					error = e;
				}
				notEq(error, null);
			}
		})
		
	})