
include
	.js('http://code.jquery.com/jquery-2.1.0.min.js')
	.done(function(){
		
		A.assert.setDOMLibrary(jQuery);
		
		UTest({
			'jquery': function(){
				
				var dom = mask.render('div > h4 > span > "Hello"');
				
				var error;
				try {
					$(dom)
						.find('h4')
						.$eq('length', 1)
						.$eq('text', 'Hello')
						.$has('html', 'span')
						;
				} catch (e) {
					error = e;
				}
				eq(error, null);
				
				var assume = assert.callback(function(){});
				try {
					
					$(dom)
						.find('h4')
						.$eq(['length', 2], 'foo')
						;
				}
				catch(error) {
					
					assume();
					notEq(error.message.indexOf('foo'), -1);
				}
			}
		})
		
	})